@model Contacto

@{
    ViewBag.Title = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="margin-top:30px">
    <div class="col-sm-12">
        <div class="panel panel-warning">
            <div class="panel-heading">
                <h4>Detalles de contacto</h4>
            </div>
            <div class="panel-body">
                <div class="form-group">
                    <input disabled type="text" id="txtIdContacto" name="IdContacto" class="form-control" value="@Html.DisplayTextFor(m => m.IdContacto)" />
                </div>
                <div class="form-group">
                    <label for="txtNombres">Nombres</label>
                    <input readonly type="text" id="txtNombres" name="Nombres" class="form-control" value="@Html.DisplayTextFor(m => m.Nombres)" />
                </div>
                <div class="form-group">
                    <label for="txtApellidos">Apellidos</label>
                    <input readonly type="text" id="txtApellidos" name="Apellidos" class="form-control" value="@Html.DisplayTextFor(m => m.Apellidos)" />
                </div>
                <div class="form-group">
                    <label for="txtTelefono">Telefono</label>
                    <input readonly type="text" id="txtTelefono" name="Telefono" class="form-control" value="@Html.DisplayTextFor(m => m.Telefono)" />
                </div>
                <div class="form-group">
                    <label for="txtCorreo">Correo</label>
                    <input readonly type="text" id="txtCorreo" name="Correo" class="form-control" value="@Html.DisplayTextFor(m => m.Correo)" />
                </div>
                <a href="@Url.Action("Index", "Contacto")" class="btn btn-primary">Volver</a>
            </div>
        </div>
    </div>
</div>

